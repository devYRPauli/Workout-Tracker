<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workouttracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Workout"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Food" />
        <activity android:name=".Health" />
        <activity android:name=".Settings" />
        <activity android:name=".Share" />
        <activity android:name=".Stats" />
        <activity android:name=".Health_water" />
        <activity android:name=".Health_active"/>
        <activity android:name=".Health_sleep"/>
    </application>

</manifest>